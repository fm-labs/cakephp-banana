{
    "name": "fm-labs/cakephp-banana",
    "description": "Banana plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Banana\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Banana\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
