{
    "name": "fm-labs/cakephp-banana",
    "description": "Banana plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "3.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Banana\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Banana\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
